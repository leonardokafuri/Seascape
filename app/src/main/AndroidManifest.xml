<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seascapehotel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PastBookings"></activity>
        <activity android:name=".Logout" />
        <activity android:name=".RoomKey" />
        <activity android:name=".DisplayBookings" />
        <activity android:name=".SuccessfulPayment" />
        <activity android:name=".Payment" />
        <activity android:name=".DisplayRooms" />
        <activity android:name=".Password" />
        <activity android:name=".SignIn" />
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity android:name=".FeedBack"/>
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="com.example.seascapehotel.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>